{
  "name": "forge-language-support",
  "displayName": "Forge Language Support",
  "description": "This extension sets grammar and keywords for the Forge file.",
  "version": "0.0.1",
  "publisher": "toastsandwich",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "forge",
      "aliases": ["Forge", "forge"],
      "extensions": [".forge", ".frg"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "forge",
      "scopeName": "source.forge",
      "path": "./syntaxes/forge.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Forge-Code-Generator/forge-language-support"
  }
}
